{
    "name": "tigerwill90/xsrf-middleware",
    "description": "Csrf protection based on double submit pattern, cookie - JWT alternative",
    "keywords" : [
      "middleware",
      "jwt",
      "branca",
      "csrf",
      "xsrf",
      "cookie",
      "auth"
    ],
    "homepage": "https://github.com/tigerwill90/xsrf-middleware",
    "license": "MIT",
    "authors": [
        {
            "name": "Sylvain Muller",
            "email": "sylvain.muller90@gmail.com",
            "homepage": "http://www.sylvainmuller.ch"
        }
    ],
    "minimum-stability": "beta",
    "require": {
      "php": "^7.1",
      "dflydev/fig-cookies": "^1.0",
      "psr/log": "^1.0",
      "psr/http-server-middleware": "^1.0",
      "psr/http-message": "^1.0",
      "tuupola/callable-handler": "^0.3",
      "tuupola/http-factory": "^0.3"

    },
    "require-dev": {
      "php": "^7.1",
      "slim/slim": "^3.9",
      "projek-xyz/slim-monolog": "^0.1",
      "phpunit/phpunit": "^7.0",
      "phpstan/phpstan": "^0.9"
    },
    "autoload": {
    "psr-4": {
        "Tigerwill90\\Middleware\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Tigerwill90\\Middleware\\": "tests"
      }
    }
}
